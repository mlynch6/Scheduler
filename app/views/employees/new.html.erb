<% provide(:title, 'New Employee') %>
<h1>New Employee</h1>
<%= form_for @employee, :html => { :class => 'form-horizontal' } do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>

	<%= render 'form', :f => f %>
	<%= render 'shared/form_submit', :f => f, :action_text => 'Create' %>
<% end %>