<% provide(:title, 'New Company Class') %>
<% provide(:header_current, 'Scheduling') %>
<% provide(:current, 'NewCompanyClass') %>

<h1>New Company Class</h1>

<div class="span9">
	<%= form_for @cclass, :html => { :class => 'form-horizontal' } do |f| %>
