<% provide(:title, 'New Costume Fitting') %>
<% provide(:header_current, 'Scheduling') %>

<h1>New Costume Fitting</h1>

<div class="row">
	<%= form_for @fitting, builder: BootstrapFormBuilder, :html => { :class => 'form-horizontal' } do |f| %>
		<%= f.error_messages %>
		<%= render 'form', :f => f %>
		<%= f.submit 'Create', :class => 'btn btn-primary' %>
	<% end %>
</div>