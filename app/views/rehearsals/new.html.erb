<% provide(:title, 'New Rehearsal') %>
<% provide(:header_current, 'Scheduling') %>
<% provide(:current, 'NewRehearsal') %>

<h1>New Rehearsal</h1>

<div class="span9">
	<%= form_for @rehearsal, builder: BootstrapFormBuilder, :html => { :class => 'form-horizontal' } do |f| %>
		<%= f.error_messages %>
		<%= render 'form', :f => f %>
		<%= f.submit 'Create', :class => 'btn btn-primary' %>
	<% end %>
</div>

<div class="span3">	
	<%= render 'shared/subnav' %>
</div>