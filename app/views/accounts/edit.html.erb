<% provide(:title, 'Company Info') %>
<h1>Company Info</h1>
<div class="row">

<div class="span8">
<%= form_for @account, :html => { :class => 'form-horizontal' } do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
	
	<div class="row">
		<div class="span8">
			<%= render 'form', :f => f %>
			
			<div class="form-actions">
				<%= f.submit "Update", :class => 'btn btn-success' %>
			</div>
		</div>
	</div>
<% end %>
</div>

<div class="span4">
	<%= render 'nav_settings', :current => 'account#edit' %>
</div>
</div>